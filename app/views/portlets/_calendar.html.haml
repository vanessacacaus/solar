#block_schedule.block_small
  .block_title
    %h2
      %i.icon-calendar
      = t(:mysolar_agenda)
  .block_content.calendar#agenda{:url_for => (url_for :agendas)}
  .calendar_events

:javascript
  //--------------------------------------------
  // Todas as datas que possuem eventos
  //--------------------------------------------
  $(function() {
    var dates_with_events = eval(#{@scheduled_events.as_json});
    showAgenda(dates_with_events);
  });